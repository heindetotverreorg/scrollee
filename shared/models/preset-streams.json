[{
    "title": "Reddit home",
    "name": "reddit-home",
    "config": {
        "meta": {
            "subreddits": []
        },
        "useLogin": true,
        "loginData": {
            "selectors": {
                "username": {
                    "selector": "#login-username",
                    "useShadowRoot": true,
                    "action": "type",
                    "value": "matthiaspoortvliet@me.com"
                },
                "password": {
                    "selector": "#login-password",
                    "useShadowRoot": true,
                    "action": "type",
                    "value": "10*Matthias",
                    "waitFor": "button.login:not([disabled])"
                },
                "loginButton": {
                    "selector": "button.login",
                    "useShadowRoot": false,
                    "action": "click"
                }
            }
        },
        "hasCookieBanner": true,
        "cookieBannerData": {
            "selectors": {
                "cookieBanner": {
                    "selector": "#accept-all-cookies-button button",
                    "useShadowRoot": true,
                    "action": "click"
                }
            }
        },
        "articleData": {
            "selectors": {
                "articles": {
                    "selector": "article",
                    "useShadowRoot": false,
                    "action": "get"
                }
            }
        },
        "refreshInterval": 10000,
        "type": "dom"
    },
    "url": "https://www.reddit.com/login"
},
